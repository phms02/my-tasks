{
  "editor.fontLigatures": true,
  "files.exclude": {
    "**/.exe": true
  },
  "material-icon-theme.folders.theme": "specific",
  "material-icon-theme.files.associations": {
    "*.ts": "typescript",
    "**.json": "json"
  },
  "code-runner.clearPreviousOutput": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook",
    "*.mind": "default"
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "diffEditor.maxComputationTime": 0,
  "python.terminal.executeInFileDir": true,
  "code-runner.executorMap": {
    "javascript": "node",
    "typescript": "ts-node",
  },
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.multiCursorPaste": "full",
  "diffEditor.renderSideBySide": false,
  "editor.inlineSuggest.enabled": true,
  "editor.snippetSuggestions": "top",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "debug.showInlineBreakpointCandidates": false,
  "editor.wordWrap": "on",
  "liveServer.settings.donotShowInfoMsg": true,
  "code-runner.saveAllFilesBeforeRun": true,
  "code-runner.saveFileBeforeRun": true,
  "terminal.integrated.fontFamily": "Fragment Mono, Roboto Mono, JetBrains Mono, monospace",
  "html.format.indentHandlebars": true,
  "debug.terminal.clearBeforeReusing": true,
  "explorer.confirmDelete": true,
  "window.closeWhenEmpty": true,
  "explorer.openEditors.visible": 10,
  "explorer.compactFolders": false,
  "editor.renderControlCharacters": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.fontFamily": "JetBrains Mono, Roboto Mono, Spline Sans Mono, monospace",
  "diffEditor.codeLens": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "liveServer.settings.donotVerifyTags": true,
  "workbench.startupEditor": "none",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDragAndDrop": false,
  "git.autofetch": true,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml"
  },
  "javascript.format.placeOpenBraceOnNewLineForFunctions": true,
  "javascript.format.placeOpenBraceOnNewLineForControlBlocks": true,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.incrementalNaming": "smart",
  "terminal.integrated.env.windows": {},
  "console-ninja.featureSet": "Community",
  "symbols.hidesExplorerArrows": false,
  "sonarlint.rules": {
    "typescript:S1135": {
      "level": "on"
    },
    "Web:TableWithoutCaptionCheck": {
      "level": "off"
    },
    "javascript:S6551": {
      "level": "on"
    }
  },
  "terminal.integrated.fontSize": 14,
  "editor.tabSize": 2,
  "editor.fontSize": 13.5,
  "console-ninja.toolsToEnableSupportAutomaticallyFor": {
    "live-server-extension": true,
    "live-preview-extension": true
  }
}